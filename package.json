{
  "name": "cuahangdientu",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "cli": "babel-node ./src/bin/cli.js",
    "data:scrape": "babel-node ./src/data/scrapeData.js",
    "data:clear": "babel-node ./src/data/clearModels.js",
    "data:import": "npx babel-node ./src/data/importData.js",
    "data:export": "npx babel-node ./src/data/exportData.js",
    "lint:fix": "eslint . --fix",
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean": "rmdir /q /s build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/index.js",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "dev-eslint": "nodemon --exec \"npm run lint:fix && babel-node ./src/index.js\""
  },
